{
	"source": {
		"includePattern": ".+\\.(js(doc|x)?|mjs)$"
	},
	"plugins": ["node_modules/jsdoc-babel"],
	"babel": {
	  "presets": [ "@babel/preset-env"],
	  "extensions": ["js", "es6", "jsx", "mjs"],
	  "plugins": [ "@babel/plugin-syntax-import-assertions"],
	  "parserOpts": {
		  "plugins": ["@babel/plugin-syntax-import-assertions"]
	  }
	}
}